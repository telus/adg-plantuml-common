' Dervived Relationships will have a lighter line and text colour [#eee]
!define Rel_Composition_Derived(e_From, e_To, e_label="") Rel_(e_From, e_To, <color:#bbb>e_label, "*-[#bbb]-")
!define Rel_Aggregation_Derived(e_From, e_To, e_label="") Rel_(e_From, e_To, <color:#bbb>e_label, "o-[#bbb]-")
!define Rel_Assignment_Derived(e_From, e_To, e_label="") Rel_(e_From, e_To, <color:#bbb>e_label, "@-[#bbb]->>")
!define Rel_Specialization_Derived(e_From, e_To, e_label="") Rel_(e_From, e_To, <color:#bbb>e_label, "-[#bbb]-|>")
!define Rel_Serving_Derived(e_From, e_To, e_label="") Rel_(e_From, e_To, <color:#bbb>e_label, "-[#bbb]->")
!define Rel_Association_Derived(e_From, e_To, e_label="") Rel_(e_From, e_To, <color:#bbb>e_label, "=[#bbb]=")
!define Rel_Flow_Derived(e_From, e_To, e_label="") Rel_(e_From, e_To, <color:#bbb>e_label, ".[#bbb].>>")
!define Rel_Realization_Derived(e_From, e_To, e_label="") Rel_(e_From, e_To, <color:#bbb>e_label, "~[#bbb]~|>")
!define Rel_Triggering_Derived(e_From, e_To, e_label="") Rel_(e_From, e_To, <color:#bbb>e_label, "-[#bbb]->>")
!define Rel_Access_Derived(e_From, e_To, e_label="") Rel_(e_From, e_To, <color:#bbb>e_label, "~[#bbb]~")
